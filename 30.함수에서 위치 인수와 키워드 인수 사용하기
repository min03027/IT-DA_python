      "cell_type": "code",
      "source": [
        "def personal_info(**kwargs):\n",
        "  if 'name' in kwargs:\n",
        "    print('name: ',kwargs['name'])\n",
        "  if 'age' in kwargs:\n",
        "    print('age: ',kwargs['age'])\n",
        "  if 'address' in kwargs:\n",
        "    print('address:',kwargs['address'])"
      ],
      "metadata": {
        "id": "40f_HlfZLijs"
      },
      "execution_count": 40,
      "outputs": []
    },
